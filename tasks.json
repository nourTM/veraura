        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(1)", "div#container header.section.header tm-editable-label"]
      },
      {
        "id": 5,
        "instruction": "Type 'Cat' and press Enter.",
        "action_type": "text_match",
        "selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(1)", "div#container header.section.header tm-editable-label", "input#label-input"],
        "expected": "Cat",
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(1)", "div#container header.section.header tm-editable-label", "input#label-input"]
      },
      {
        "id": 6,
        "instruction": "Now, click on 'Class 2' to rename it.",
        "action_type": "click",
        "selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(2)", "div#container header.section.header tm-editable-label"],
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(2)", "div#container header.section.header tm-editable-label"]
      },
      {
        "id": 7,
        "instruction": "Type 'Not Cat' and press Enter.",
        "action_type": "text_match",
        "selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(2)", "div#container header.section.header tm-editable-label", "input#label-input"],
        "expected": "Not Cat",
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(2)", "div#container header.section.header tm-editable-label", "input#label-input"]
      },
      {
        "id": 8,
        "instruction": "Click 'Upload' to add your cat images. Be sure to add 5-10 images from different angles, colors, and sizes.",
