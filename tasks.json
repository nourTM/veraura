  "teachable_machine_02": {
    "title": "Teachable Machine Interaction Lab ",
    "start_url": "https://teachablemachine.withgoogle.com/",
    "steps": [
      {
        "id": 1,
        "instruction": "Find the 'Get Started' button and click it.",
        "action_type": "click",
        "selector": "a.button[href='/train']",
        "highlight_selector": "a.button[href='/train']"
      },
      {
        "id": 2,
        "instruction": "Click on the 'Image Project' option to create a new project.",
        "action_type": "click",
        "selector": ["tm-new-model-card#image-card", "#main-project"],
        "highlight_selector": "tm-new-model-card#image-card"
      },

      {
        "id": 3,
        "instruction": "Click on the 'Standard Image Model' option in the sub-project modal.",
        "action_type": "click",
        "selector": ["tm-new-model-card#image-card", "tm-modal#sub-project-modal .project.primary-project"],
        "highlight_selector": ["tm-new-model-card#image-card", "tm-modal#sub-project-modal .project.primary-project"]
      },
      {
        "id": 4,
        "instruction": "Click on 'Class 1' to rename it.",
