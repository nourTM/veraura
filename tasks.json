        "selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(1)", "div#container div#open-container div#sample-input-list button[title='Add sample: upload']"],
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(1)", "div#container div#open-container div#sample-input-list button[title='Add sample: upload']"]
      },
      {
        "id": 9,
        "instruction": "Great! The guide will continue automatically once at least 5 samples have been added.",
        "action_type": "text_change_match",
        "selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(1)", "div#container div#open-samples-label"],
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(1)", "div#container div#open-samples-label"],
        "expected": "(?:[5-9]|[1-9]\\d+)\\s+Image Sample(s?)"
      },
      {
        "id": 10,
        "instruction": "Now, for the 'Not Cat' class, click 'Upload'. Add 5-10 varied images (e.g., dogs, people, objects).",
        "action_type": "click",
        "selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(2)", "div#container div#open-container div#sample-input-list button[title='Add sample: upload']"],
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(2)", "div#container div#open-container div#sample-input-list button[title='Add sample: upload']"]
      },
      {
        "id": 11,
        "instruction": "The guide will continue once at least 5 samples are added to the 'Not Cat' class.",
        "action_type": "text_change_match",
        "selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(2)", "div#container div#open-samples-label"],
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-classifier-list#classifier-list tm-classifier-drawer:nth-of-type(2)", "div#container div#open-samples-label"],
        "expected": "(?:[5-9]|[1-9]\\d+)\\s+Image Sample(s?)"
      },
      {
        "id": 12,
        "instruction": "Click 'Train Model' and wait for the training to complete (this may take 30-60 seconds).",
        "action_type": "click",
        "selector": ["tm-teachable-image-app#tmApp", "tm-train#train", "tm-progress-button#train-btn"],
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-train#train", "tm-progress-button#train-btn"]
      },
      {
        "id": 13,
        "instruction": "Great job! In the 'Preview' panel, select 'File' or 'Webcam' to test your model and see how it performs.",
        "action_type": "click",
        "selector": ["tm-teachable-image-app#tmApp", "tm-run#run", "select#select-input"],
        "highlight_selector": ["tm-teachable-image-app#tmApp", "tm-run#run", "select#select-input"]
      }
